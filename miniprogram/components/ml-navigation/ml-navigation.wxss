/* components/ml-navigation/ml-navigation.wxss */
@font-face {
  font-family: 'iconfont';
  src: url('//at.alicdn.com/t/font_529957_e5dwkxb7fna.eot?t=1595759434011');
  /* IE9 */
  src: url('//at.alicdn.com/t/font_529957_e5dwkxb7fna.eot?t=1595759434011#iefix') format('embedded-opentype'), /* IE6-IE8 */ url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'), url('//at.alicdn.com/t/font_529957_e5dwkxb7fna.woff?t=1595759434011') format('woff'), url('//at.alicdn.com/t/font_529957_e5dwkxb7fna.ttf?t=1595759434011') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */ url('//at.alicdn.com/t/font_529957_e5dwkxb7fna.svg?t=1595759434011#iconfont') format('svg');
  /* iOS 4.1- */
}
.iconfont {
  font-family: 'iconfont' !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-home_light:before {
  content: '\e7d4';
}
.icon-move:before {
  content: '\e768';
}
.icon-back_light:before {
  content: '\e7e0';
}
.ml-navigation .navigation-view {
  width: 100vw !important;
  padding: 0;
  margin: 0;
  position: relative;
  box-sizing: border-box;
  border-radius: 0;
}
.ml-navigation .navigation-view.fixed-view {
  position: fixed;
  top: 0;
  left: 0;
}
.ml-navigation .navigation-view .inner {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  font-weight: bold;
}
.ml-navigation .navigation-view .icon-view {
  position: absolute;
  display: flex;
}
.ml-navigation .navigation-view .icon-view .iconfont {
  font-size: 40rpx;
}
.ml-navigation .navigation-view .icon-view .icon-move {
  font-size: 40rpx;
  transform: rotate(90deg);
  font-weight: lighter;
  opacity: 0.3;
}
