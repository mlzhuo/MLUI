page {
  background: #f6f6f6;
  font-size: 32rpx;
  color: #353535;
  overflow-x: hidden;
}
button {
  background: initial;
}
button:focus {
  outline: 0;
}
button::after {
  border: none;
}
/* color */
.black {
  color: #353535;
}
.blue {
  color: #69cdff;
}
.green {
  color: #4bd197;
}
.red {
  color: #ff5c5c;
}
.orange {
  color: #ffcb47;
}
.purple {
  color: #ff8ccb;
}
.white {
  color: #ffffff;
}
/* font */
.normal {
  font-weight: normal;
}
.bold {
  font-weight: bold;
}
.text-center {
  text-align: center;
}
.text-left {
  text-align: left;
}
.text-right {
  text-align: right;
}
.font-12 {
  font-size: 24rpx;
}
.font-14 {
  font-size: 28rpx;
}
.font-16 {
  font-size: 32rpx;
}
.font-18 {
  font-size: 36rpx;
}
.font-20 {
  font-size: 40rpx;
}
/* 文字溢出处理 2行 + ... */
.one-line-text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.two-lines-text {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
/* background */
.bg-black {
  background-color: #353535;
}
.bg-blue {
  background-color: #69cdff;
}
.bg-green {
  background-color: #4bd197;
}
.bg-red {
  background-color: #ff5c5c;
}
.bg-orange {
  background-color: #ffcb47;
}
.bg-purple {
  background-color: #ff8ccb;
}
.bg-white {
  background-color: #ffffff;
}
/* border */
.radius {
  border-radius: 8rpx;
}
.border {
  border: 2rpx solid #f0f0f0;
}
.no-border {
  border: none;
}
.border-black {
  border-color: #353535;
}
.border-blue {
  border-color: #69cdff;
}
.border-green {
  border-color: #4bd197;
}
.border-red {
  border-color: #ff5c5c;
}
.border-orange {
  border-color: #ffcb47;
}
.border-purple {
  border-color: #ff8ccb;
}
.border-white {
  border-color: #ffffff;
}
.round {
  border-radius: 50%;
}
/* flex */
.flex {
  display: flex;
}
.flex.colomn {
  flex-direction: column;
}
.flex.center {
  align-items: center;
}
.flex.end {
  align-items: flex-end;
}
.flex.around {
  justify-content: space-around;
}
.flex.between {
  justify-content: space-between;
}
.scroll-x {
  overflow-y: hidden;
  overflow-x: auto;
}
/* position */
.relative {
  position: relative;
}
.absolute {
  position: absolute;
}
.fixed {
  position: fixed;
}
.position-center {
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
.ml-banner .inner {
  transition: left 0.3s ease;
}
.ml-banner .inner .banner-item {
  height: 100%;
  display: inline-block;
}
.ml-banner .inner .banner-item image {
  width: 100%;
  height: 100%;
}
.ml-banner .inner .banner-item video {
  width: 100%;
  height: 100%;
}
.ml-banner .point {
  bottom: 5%;
  left: 50vw;
  transform: translateX(-50%);
}
.ml-banner .point .point-item {
  height: 10rpx;
  width: 10rpx;
  margin: 0 10rpx;
  border-radius: 5rpx;
  transition: width 0.3s ease;
}
.ml-banner .point .point-item.active {
  width: 40rpx;
}
