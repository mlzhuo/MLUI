page {
  background: #f6f6f6;
  font-size: 32rpx;
  color: #000;
  overflow-x: hidden;
}
button {
  background: initial;
}
button:focus {
  outline: 0;
}
button::after {
  border: none;
}
/* color */
.black {
  color: #000;
}
.blue {
  color: #69cdff;
}
.green {
  color: #4bd197;
}
.red {
  color: #ff5c5c;
}
.orange {
  color: #ffcb47;
}
.purple {
  color: #ff8ccb;
}
.white {
  color: #ffffff;
}
.gray {
  color: #f0f0f0;
}
/* font */
.normal {
  font-weight: normal;
}
.bold {
  font-weight: bold;
}
.text-center {
  text-align: center;
}
.text-left {
  text-align: left;
}
.text-right {
  text-align: right;
}
.font-12 {
  font-size: 24rpx;
}
.font-14 {
  font-size: 28rpx;
}
.font-16 {
  font-size: 32rpx;
}
.font-18 {
  font-size: 36rpx;
}
.font-20 {
  font-size: 40rpx;
}
/* 文字溢出处理 2行 + ... */
.one-line-text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.two-lines-text {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
/* background */
.bg-black {
  background-color: #000;
}
.bg-blue {
  background-color: #69cdff;
}
.bg-green {
  background-color: #4bd197;
}
.bg-red {
  background-color: #ff5c5c;
}
.bg-orange {
  background-color: #ffcb47;
}
.bg-purple {
  background-color: #ff8ccb;
}
.bg-white {
  background-color: #ffffff;
}
.bg-gray {
  background-color: #f0f0f0;
}
/* border */
.radius {
  border-radius: 8rpx;
}
.border {
  border: 2rpx solid #f0f0f0;
}
.no-border {
  border: none;
}
.border-black {
  border-color: #000;
}
.border-blue {
  border-color: #69cdff;
}
.border-green {
  border-color: #4bd197;
}
.border-red {
  border-color: #ff5c5c;
}
.border-orange {
  border-color: #ffcb47;
}
.border-purple {
  border-color: #ff8ccb;
}
.border-white {
  border-color: #ffffff;
}
.round {
  border-radius: 50%;
}
/* flex */
.flex {
  display: flex;
}
.flex.colomn {
  flex-direction: column;
}
.flex.center {
  align-items: center;
}
.flex.end {
  align-items: flex-end;
}
.flex.space-around {
  justify-content: space-around;
}
.flex.space-between {
  justify-content: space-between;
}
.flex.justify-start {
  justify-content: flex-start;
}
.flex.justify-end {
  justify-content: flex-end;
}
.scroll-x {
  overflow-y: hidden;
  overflow-x: auto;
}
/* position */
.relative {
  position: relative;
}
.absolute {
  position: absolute;
}
.fixed {
  position: fixed;
}
.position-center {
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
.ml-timeline .timeline-item .content .left-icon {
  width: 80rpx;
}
.ml-timeline .timeline-item .content .left-icon .icon {
  display: inline-block;
  margin: 0 auto;
}
.ml-timeline .timeline-item .content .left-icon .line {
  flex: 1;
  display: inline-block;
  width: 2rpx;
  background-color: rgba(0, 0, 0, 0.5);
  margin: 0 auto;
}
.ml-timeline .timeline-item .content .right-content {
  flex: 1;
  font-size: 32rpx;
  padding-bottom: 20rpx;
  box-sizing: border-box;
}
.ml-timeline .timeline-item .content .right-content .content-inner {
  padding: 20rpx;
}
