/* components/calendar/index.wxss */
/**mlui.wxss**/
@font-face {
  font-family: 'iconfont';
  src: url('//at.alicdn.com/t/font_529957_e5dwkxb7fna.eot?t=1595759434011');
  /* IE9 */
  src: url('//at.alicdn.com/t/font_529957_e5dwkxb7fna.eot?t=1595759434011#iefix') format('embedded-opentype'), /* IE6-IE8 */ url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'), url('//at.alicdn.com/t/font_529957_e5dwkxb7fna.woff?t=1595759434011') format('woff'), url('//at.alicdn.com/t/font_529957_e5dwkxb7fna.ttf?t=1595759434011') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */ url('//at.alicdn.com/t/font_529957_e5dwkxb7fna.svg?t=1595759434011#iconfont') format('svg');
  /* iOS 4.1- */
}
.iconfont {
  font-family: 'iconfont' !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-home_light:before {
  content: '\e7d4';
}
.icon-move:before {
  content: '\e768';
}
.icon-back_light:before {
  content: '\e7e0';
}
page {
  background: #f6f6f6;
  font-size: 32rpx;
  color: #353535;
}
button {
  background: initial;
}
button:focus {
  outline: 0;
}
button::after {
  border: none;
}
/* color */
.black {
  color: #353535;
}
.blue {
  color: #69cdff;
}
.green {
  color: #4bd197;
}
.red {
  color: #ff5c5c;
}
.orange {
  color: #ffcb47;
}
.purple {
  color: #ff8ccb;
}
.white {
  color: #ffffff;
}
/* font */
.normal {
  font-weight: normal;
}
.bold {
  font-weight: bold;
}
.text-center {
  text-align: center;
}
.text-left {
  text-align: left;
}
.text-right {
  text-align: right;
}
/* background */
.bg-black {
  background-color: #353535;
}
.bg-blue {
  background-color: #69cdff;
}
.bg-green {
  background-color: #4bd197;
}
.bg-red {
  background-color: #ff5c5c;
}
.bg-orange {
  background-color: #ffcb47;
}
.bg-purple {
  background-color: #ff8ccb;
}
.bg-white {
  background-color: #ffffff;
}
/* border */
.radius {
  border-radius: 8rpx;
}
.border {
  border: 2rpx solid #f0f0f0;
}
.no-border {
  border: none;
}
.border-black {
  border-color: #353535;
}
.border-blue {
  border-color: #69cdff;
}
.border-green {
  border-color: #4bd197;
}
.border-red {
  border-color: #ff5c5c;
}
.border-orange {
  border-color: #ffcb47;
}
.border-purple {
  border-color: #ff8ccb;
}
.border-white {
  border-color: #ffffff;
}
.round {
  border-radius: 50%;
}
/* flex */
.flex {
  display: flex;
}
.flex.colomn {
  flex-direction: column;
}
.flex.center {
  align-items: center;
}
.flex.end {
  align-items: flex-end;
}
.flex.around {
  justify-content: space-around;
}
.flex.between {
  justify-content: space-between;
}
.scroll-x {
  overflow-y: hidden;
  overflow-x: auto;
}
/* position */
.relative {
  position: relative;
}
.absolute {
  position: absolute;
}
.ml-calendar {
  min-height: 370rpx;
  padding: 24rpx;
}
.ml-calendar .week-item {
  height: 50rpx;
  line-height: 50rpx;
  width: 80rpx;
}
.ml-calendar .day-item {
  height: 80rpx;
  line-height: 80rpx;
  width: 80rpx;
  overflow: hidden;
  margin-top: 24rpx;
  box-sizing: border-box;
}
.ml-calendar .day-item .day-flag {
  width: 8rpx;
  height: 8rpx;
  left: 50%;
  transform: translateX(-50%);
  bottom: 5rpx;
}
