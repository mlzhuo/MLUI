/**index.wxss**/
.ml-gird {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: center;
  background: #fff;
}
.ml-gird .ml-gird-item {
  width: 33%;
  height: 160rpx;
  line-height: 160rpx;
  text-align: center;
  box-sizing: border-box;
}
.ml-gird-item-center {
  border-left: 2rpx solid #efefef;
  border-right: 2rpx solid #efefef;
}
